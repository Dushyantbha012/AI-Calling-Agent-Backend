# Backend Server Configuration
SERVER=your_server_here
PORT=8080

# Service API Keys

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
OWNER_PHONE_NUMBER=your_owner_phone_number
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# AI Services
## LLM
OPENAI_API_KEY=your_openai_api_key
GROQ_API_KEY=your_groq_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

## Speech Understanding/TTS
DEEPGRAM_API_KEY=your_deepgram_api_key

## Google Search (Optional)
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id

## TTS
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_VOICE_ID=your_voice_id

# Service Selection
TTS_SERVICE=deepgram
LLM_SERVICE=groq

# Phone Configuration
APP_NUMBER=your_app_number
YOUR_NUMBER=your_number
TRANSFER_NUMBER=your_transfer_number

# AI Configuration
SYSTEM_MESSAGE="You are an AI phone agent tasked with supporting the user in his queries"
INITIAL_MESSAGE="Hi there, can I help you with something?"

# Call Recording
RECORD_CALLS=false

# Owner Features
ENABLE_OWNER_FEATURES=true
OWNER_EMAIL=your_owner_email@example.com

# Gmail Configuration
GMAIL_SENDER_NAME="AI Assistant"

# Email Configuration
SENDER_EMAIL=your_sender_email
SENDER_PASSWORD=your_sender_password
SMTP_SERVER=smtp-relay.brevo.com
SMTP_PORT=587

# Qdrant Vector Database Configuration
QDRANT_HOST=2c3e3d22-5d50-4176-8bc4-e5397dd77c56.us-east4-0.gcp.cloud.qdrant.io
QDRANT_PORT=6333
QDRANT_API_KEY=your_qdrant_api_key
QDRANT_COLLECTION_NAME=phone_conversations
QDRANT_VECTOR_SIZE=1536

# RAG Configuration
RAG_ENABLED=true
RAG_MAX_CONTEXT_CHUNKS=5
RAG_SIMILARITY_THRESHOLD=0.7
RAG_CHUNK_SIZE=500
RAG_CHUNK_OVERLAP=50

# System Configuration
TOKENIZERS_PARALLELISM=false
